{
  "$schema": "https://json.schemastore.org/chrome-manifest",
  "manifest_version": 3,
  "name": "Blockly",
  "description": "Block distracting websites",
  "version": "0.1.0",
  "icons": {
    "48": "assets/icon-48.png"
  },
  "options_page": "entry/options.html",
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://*/*", "http://*/*"]
}
